{
    "name": "@camberi/firecms",
    "version": "1.0.0-rc.5",
    "description": "Awesome Firebase/Firestore-based headless open-source CMS",
    "author": "camberi",
    "homepage": "https://firecms.co",
    "license": "MIT",
    "repository": "git@github.com:Camberi/firecms.git",
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "types": "dist/index.d.ts",
    "source": "src/index.ts",
    "engines": {
        "node": ">=14"
    },
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        }
    },
    "keywords": [
        "firebase",
        "cms",
        "admin",
        "admin panel",
        "firebase panel",
        "firestore",
        "headless",
        "headless cms",
        "content manager"
    ],
    "scripts": {
        "dev": "cd example && run-s dev",
        "build": "vite build && tsc --emitDeclarationOnly",
        "prepublishOnly": "run-s build",
        "createTag": "PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags",
        "eslint": "eslint \"src/**\"",
        "test": "run-s test:unit test:lint",
        "test:lint": "eslint \"src/**\"",
        "test:unit": "cross-env CI=1 react-scripts test --transformIgnorePatterns \"node_modules/(?!react-markdown)/\"",
        "test:watch": "react-scripts test --transformIgnorePatterns \"node_modules/(?!react-markdown)/\"",
        "precommit": "build"
    },
    "dependencies": {
        "@date-io/date-fns": "^2.11.0",
        "@uiw/react-md-editor": "^3.9.3",
        "date-fns": "^2.21.3",
        "formik": "^2.2.9",
        "history": "^5",
        "object-hash": "^2.2.0",
        "react-base-table": "^1.12.0",
        "react-beautiful-dnd": "^13.1.0",
        "react-dropzone": "^11.4.2",
        "react-measure": "^2.5.2",
        "react-transition-group": "^4.4.1",
        "typeface-roboto": "^1.1.13",
        "typeface-rubik": "^1.1.13",
        "typeface-space-mono": "^1.1.13",
        "yup": "^0.32.9"
    },
    "peerDependencies": {
        "@emotion/react": "^11.4.1",
        "@emotion/styled": "^11.3.0",
        "@mui/icons-material": "^5",
        "@mui/lab": "^5.0.0-alpha.48",
        "@mui/material": "^5.0.0",
        "@mui/styles": "^5.0.0",
        "algoliasearch": "^4.9.1",
        "firebase": "^9.0.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router": "^6.2.0",
        "react-router-dom": "^6.2.0",
        "react-scripts": "^5.0.0"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.16.5",
        "@emotion/react": "^11.7.1",
        "@emotion/styled": "^11.6.0",
        "@mui/icons-material": "^5",
        "@mui/lab": "^5.0.0-alpha.67",
        "@mui/material": "^5",
        "@mui/styles": "^5",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.0.0",
        "@testing-library/user-event": "^13.2.1",
        "@types/jest": "^27.4.0",
        "@types/node": "^16.7.13",
        "@types/object-hash": "^2.1.0",
        "@types/react": "^17.0.20",
        "@types/react-beautiful-dnd": "^13.1.2",
        "@types/react-dom": "^17.0.9",
        "@types/react-measure": "^2.0.6",
        "@typescript-eslint/eslint-plugin": "^5.9.0",
        "@typescript-eslint/parser": "^5.9.0",
        "@vitejs/plugin-react": "^1.3.0",
        "algoliasearch": "^4.9.1",
        "cross-env": "^7.0.3",
        "eslint": "^8.6.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-config-standard-react": "^11.0.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "firebase": "^9.4.1",
        "jest": "^27.4.7",
        "npm-run-all": "^4.1.5",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router": "^6.2.1",
        "react-router-dom": "^6.2.1",
        "react-scripts": "^5.0.0",
        "tsd": "^0.19.1",
        "typescript": "^4.5.4",
        "vite": "^2.9.1"
    },
    "files": [
        "dist"
    ]
}
